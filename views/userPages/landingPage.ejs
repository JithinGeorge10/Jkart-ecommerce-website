  <!-- BANNER SLIDER -->

  <script>
    $(document).ready(function () {
      $('.banner_slider').slick({
        slidesToShow: 1,
        slidesToScroll: 1,
        autoplay: true,
        autoplaySpeed: 2000
      })
    }) // Change the sliding
  </script>


  <section class="banner_slider_wrapper clearfix">
    <div class="container">
      <div class="banner_slider">
        <div>
          <div class="row align-items-center">
            <div class="col-6">
              <div class="content">
                <h6>Limited time offer!</h6>
                <h1>NIKE AIR</h1>
                <p class="text-truncate">
                  Enjoy our limited time offer with huge discounts on Nike shoes! Don't miss out on this opportunity to
                  grab your favorite styles at unbeatable prices.
                </p>
                <a href="/shop" class="btn btn-primary-outlined btn-sm">
                  Buy Now <span><i class="fa fa-chevron-right"></i></span>
                </a>
              </div>
            </div>
            <div class="col-6">
              <picture class="image">
                <img src="assets/img/home/banner-slider/sl1-sm.png" style="width: 500px;" alt="slider-image">
              </picture>
            </div>
          </div>
        </div>
        <!-- slider items -->
        <div>
          <div class="row align-items-center">
            <div class="col-6">
              <picture class="image">
                <img class="img-fluid" src="assets/img/home/banner-slider/sl2-sm.png" style="width: 500px;"
                  alt="slider-image">
              </picture>
            </div>
            <div class="col-6">
              <div class="content">
                <h6>Limited time offer!</h6>
                <h1>Running Shoes</h1>
                <p class="text-truncate">
                  Nike running shoes are designed to provide a snug and comfortable fit, with options available in
                  various widths to accommodate different foot shapes.
                </p>
                <a href="/shop" class="btn btn-primary-outlined btn-sm">
                  Buy Now <span><i class="fa fa-chevron-right"></i></span>
                </a>
              </div>
            </div>
          </div>
        </div>
        <div>
          <div class="row align-items-center">
            <div class="col-6">
              <div class="content">
                <h6>Limited time offer!</h6>
                <h1>Sneakers</h1>
                <p class="text-truncate">
                  Hey sneakerheads! It's time to lace up and sprint towards savings! For a limited time only, we're
                  offering jaw-dropping discounts on your favorite Nike sneakers
                </p>
                <a href="/shop" class="btn btn-primary-outlined btn-sm">
                  Buy Now <span><i class="fa fa-chevron-right"></i></span>
                </a>
              </div>
            </div>
            <div class="col-6">
              <picture class="image">
                <img class="img-fluid" src="assets/img/home/banner-slider/shoe1-sm.png" style="width: 800px;"
                  alt="slider-image">
              </picture>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>



